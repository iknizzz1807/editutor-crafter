runtime_values."Data Model"."Type Relationships": {
  shape: rectangle
  style.fill: "#0f3460"
  style.stroke: "#3fb950"
  style.font-color: "#e6edf3"

  runtime_value: Runtime Value {
    shape: class
    class: abstract_type
    - "type: string"
  }

  number: Number {
    shape: class
    class: concrete_type
    + "value: number"
  }

  string: String {
    shape: class
    class: concrete_type
    + "value: string"
  }

  boolean: Boolean {
    shape: class
    class: concrete_type
    + "value: boolean"
  }

  nil: Nil {
    shape: class
    class: concrete_type
    + "value: null"
  }

  function: Function {
    shape: class
    class: concrete_type
    + "parameters: string array"
    + "body: BlockStatement"
    + "closure_environment: Environment"
  }

  environment: Environment {
    shape: class
    class: supporting_class
    + "parent: Environment | null"
    + "store: Map<string, Runtime Value>"
  }

  runtime_value -> number: is_a
  runtime_value -> string: is_a
  runtime_value -> boolean: is_a
  runtime_value -> nil: is_a
  runtime_value -> function: is_a
  function -> environment: references {
    target-arrowhead.shape: diamond
  }
  environment -> environment: parent {
    label: optional
    target-arrowhead.shape: diamond
  }
}

classes: {
  abstract_type: {
    style.fill: "#1a1a2e"
    style.stroke: "#8b949e"
    style.double-border: true
    style.bold: true
    style.italic: true
  }
  concrete_type: {
    style.fill: "#16213e"
    style.stroke: "#3fb950"
    style.bold: true
  }
  supporting_class: {
    style.fill: "#1a1a2e"
    style.stroke: "#e6a700"
  }
}