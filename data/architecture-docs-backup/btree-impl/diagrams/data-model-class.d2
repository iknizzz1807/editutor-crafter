title: B-tree Data Model

classes: {
  container_style: {
    style.fill: "#0f3460"
    style.stroke: "#3fb950"
    style.font-color: "#e6edf3"
  }
  class_style: {
    style.fill: "#16213e"
    style.stroke: "#8b949e"
    style.font-color: "#e6edf3"
    style.double-border: false
  }
  important_class: {
    style.fill: "#1a1a2e"
    style.stroke: "#3fb950"
    style.font-color: "#ffffff"
    style.bold: true
  }
}

b_tree: {
  class: container_style
  label: "BTree Data Model"

  btree_struct: {
    class: important_class
    shape: class
    - "root: BTreeNode*"
    - "t: int"
  }

  btnode_struct: {
    class: class_style
    shape: class
    - "keys: int array"
    - "children: BTreeNode* array"
    - "num_keys: int"
    - "is_leaf: bool"
  }

  btree_struct -> btnode_struct: "has root"
  btnode_struct -> btnode_struct: "has child nodes"
}