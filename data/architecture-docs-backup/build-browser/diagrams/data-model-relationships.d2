direction: right

# Core Data Types
DOMNode: DOM Node {
  shape: class
  + tag: string
  + attributes: Map<string, string>
  + children: List<DOMNode>
  + parent: DOMNode?
  style.fill: "#1a1a2e"
  style.stroke: "#3fb950"
  style.font-color: "#e6edf3"
}

Element: Element {
  shape: class
  + tagName: string
  + id: string?
  + className: string?
  + innerHTML: string
  style.fill: "#1a1a2e"
  style.stroke: "#3fb950"
  style.font-color: "#e6edf3"
}

TextNode: Text Node {
  shape: class
  + content: string
  + whiteSpace: WhitespaceType
  style.fill: "#1a1a2e"
  style.stroke: "#3fb950"
  style.font-color: "#e6edf3"
}

CSSRule: CSS Rule {
  shape: class
  + selector: Selector
  + declarations: List<Declaration>
  + specificity: int
  + source: StylesheetOrigin
  style.fill: "#16213e"
  style.stroke: "#3fb950"
  style.font-color: "#e6edf3"
}

ComputedStyle: Computed Style {
  shape: class
  + display: DisplayType
  + position: PositionType
  + dimensions: BoxDimensions
  + colors: ColorProperties
  + fonts: FontProperties
  style.fill: "#0f3460"
  style.stroke: "#3fb950"
  style.font-color: "#e6edf3"
  style.bold: true
}

LayoutBox: Layout Box {
  shape: class
  + rect: Rectangle
  + node: StyledNode
  + children: List<LayoutBox>
  + boxType: BoxType
  style.fill: "#16213e"
  style.stroke: "#3fb950"
  style.font-color: "#e6edf3"
}

PaintCommand: Paint Command {
  shape: class
  + commandType: PaintType
  + bounds: Rectangle
  + style: RenderStyle
  + zIndex: int
  style.fill: "#0f3460"
  style.stroke: "#3fb950"
  style.font-color: "#e6edf3"
  style.bold: true
}

# Relationships
DOMNode -> Element: extends {
  style.stroke: "#8b949e"
}

DOMNode -> TextNode: extends {
  style.stroke: "#8b949e"
}

Element -> ComputedStyle: styled with {
  style.stroke: "#8b949e"
}

CSSRule -> ComputedStyle: contributes to {
  style.stroke: "#8b949e"
}

ComputedStyle -> LayoutBox: layout calculation {
  style.stroke: "#8b949e"
}

Element -> LayoutBox: positioned as {
  style.stroke: "#8b949e"
}

LayoutBox -> PaintCommand: generates {
  style.stroke: "#8b949e"
}

DOMNode -> DOMNode: parent-child {
  style.stroke: "#8b949e"
  style.stroke-dash: 5
}

LayoutBox -> LayoutBox: box hierarchy {
  style.stroke: "#8b949e"
  style.stroke-dash: 5
}