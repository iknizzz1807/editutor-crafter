title: HTML Parsing State Machine {
  style.font-size: 20
  style.font-color: "#e6edf3"
}

data_state: Data State {
  shape: circle
  style.fill: "#1a1a2e"
  style.stroke: "#3fb950"
  style.font-color: "#e6edf3"
}

tag_open_state: Tag Open State {
  shape: circle
  style.fill: "#1a1a2e"
  style.stroke: "#3fb950"
  style.font-color: "#e6edf3"
}

tag_name_state: Tag Name State {
  shape: circle
  style.fill: "#1a1a2e"
  style.stroke: "#3fb950"
  style.font-color: "#e6edf3"
}

before_attribute_name_state: Before Attribute Name {
  shape: circle
  style.fill: "#1a1a2e"
  style.stroke: "#3fb950"
  style.font-color: "#e6edf3"
}

attribute_name_state: Attribute Name State {
  shape: circle
  style.fill: "#1a1a2e"
  style.stroke: "#3fb950"
  style.font-color: "#e6edf3"
}

before_attribute_value_state: Before Attribute Value {
  shape: circle
  style.fill: "#1a1a2e"
  style.stroke: "#3fb950"
  style.font-color: "#e6edf3"
}

attribute_value_quoted_state: Attribute Value (Quoted) {
  shape: circle
  style.fill: "#1a1a2e"
  style.stroke: "#3fb950"
  style.font-color: "#e6edf3"
}

after_attribute_value_state: After Attribute Value {
  shape: circle
  style.fill: "#1a1a2e"
  style.stroke: "#3fb950"
  style.font-color: "#e6edf3"
}

end_tag_open_state: End Tag Open {
  shape: circle
  style.fill: "#1a1a2e"
  style.stroke: "#3fb950"
  style.font-color: "#e6edf3"
}

end_tag_name_state: End Tag Name {
  shape: circle
  style.fill: "#1a1a2e"
  style.stroke: "#3fb950"
  style.font-color: "#e6edf3"
}

initial: Start {
  shape: circle
  style.fill: "#3fb950"
  style.stroke: "#3fb950"
  style.font-color: "#000000"
}

initial -> data_state: {
  style.stroke: "#8b949e"
}

data_state -> tag_open_state: "<" {
  style.stroke: "#8b949e"
  style.font-color: "#e6edf3"
}

tag_open_state -> tag_name_state: "a-z, A-Z" {
  style.stroke: "#8b949e"
  style.font-color: "#e6edf3"
}

tag_open_state -> end_tag_open_state: "/" {
  style.stroke: "#8b949e"
  style.font-color: "#e6edf3"
}

tag_name_state -> before_attribute_name_state: "space" {
  style.stroke: "#8b949e"
  style.font-color: "#e6edf3"
}

tag_name_state -> data_state: ">" {
  style.stroke: "#8b949e"
  style.font-color: "#e6edf3"
}

before_attribute_name_state -> attribute_name_state: "a-z, A-Z" {
  style.stroke: "#8b949e"
  style.font-color: "#e6edf3"
}

before_attribute_name_state -> data_state: ">" {
  style.stroke: "#8b949e"
  style.font-color: "#e6edf3"
}

attribute_name_state -> before_attribute_value_state: "=" {
  style.stroke: "#8b949e"
  style.font-color: "#e6edf3"
}

attribute_name_state -> before_attribute_name_state: "space" {
  style.stroke: "#8b949e"
  style.font-color: "#e6edf3"
}

before_attribute_value_state -> attribute_value_quoted_state: "\"" {
  style.stroke: "#8b949e"
  style.font-color: "#e6edf3"
}

attribute_value_quoted_state -> after_attribute_value_state: "\"" {
  style.stroke: "#8b949e"
  style.font-color: "#e6edf3"
}

after_attribute_value_state -> before_attribute_name_state: "space" {
  style.stroke: "#8b949e"
  style.font-color: "#e6edf3"
}

after_attribute_value_state -> data_state: ">" {
  style.stroke: "#8b949e"
  style.font-color: "#e6edf3"
}

end_tag_open_state -> end_tag_name_state: "a-z, A-Z" {
  style.stroke: "#8b949e"
  style.font-color: "#e6edf3"
}

end_tag_name_state -> data_state: ">" {
  style.stroke: "#8b949e"
  style.font-color: "#e6edf3"
}

data_state -> data_state: "text chars" {
  style.stroke: "#8b949e"
  style.font-color: "#e6edf3"
}

tag_name_state -> tag_name_state: "a-z, A-Z, 0-9" {
  style.stroke: "#8b949e"
  style.font-color: "#e6edf3"
}

attribute_name_state -> attribute_name_state: "a-z, A-Z, 0-9, -" {
  style.stroke: "#8b949e"
  style.font-color: "#e6edf3"
}

attribute_value_quoted_state -> attribute_value_quoted_state: "any except \"" {
  style.stroke: "#8b949e"
  style.font-color: "#e6edf3"
}

end_tag_name_state -> end_tag_name_state: "a-z, A-Z, 0-9" {
  style.stroke: "#8b949e"
  style.font-color: "#e6edf3"
}