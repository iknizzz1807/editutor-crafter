shape: sequence_diagram

layout_engine: Layout Engine {
  style.fill: "#1a1a2e"
  style.stroke: "#3fb950"
  style.font-color: "#e6edf3"
  style.bold: true
}

paint_engine: Paint Engine {
  style.fill: "#16213e"
  style.stroke: "#3fb950"
  style.font-color: "#e6edf3"
  style.bold: true
}

display_list: Display List {
  style.fill: "#0f3460"
  style.stroke: "#3fb950"
  style.font-color: "#e6edf3"
  shape: page
}

graphics_backend: Graphics Backend {
  style.fill: "#1a1a2e"
  style.stroke: "#3fb950"
  style.font-color: "#e6edf3"
  style.bold: true
}

layout_engine -> paint_engine: traverse(root_element) {
  style.stroke: "#8b949e"
}

paint_engine -> paint_engine: process_element(elem) {
  style.stroke: "#8b949e"
}

paint_engine -> display_list: add_command(paint_background) {
  style.stroke: "#3fb950"
  style.bold: true
}

paint_engine -> display_list: add_command(paint_border) {
  style.stroke: "#3fb950"
  style.bold: true
}

paint_engine -> paint_engine: traverse_children(elem) {
  style.stroke: "#8b949e"
}

paint_engine -> display_list: add_command(paint_text) {
  style.stroke: "#3fb950"
  style.bold: true
}

paint_engine -> paint_engine: sort_by_z_order() {
  style.stroke: "#8b949e"
}

paint_engine -> layout_engine: traversal_complete {
  style.stroke: "#8b949e"
}

layout_engine -> graphics_backend: execute_display_list() {
  style.stroke: "#3fb950"
  style.bold: true
}

graphics_backend -> display_list: get_next_command() {
  style.stroke: "#8b949e"
}

display_list -> graphics_backend: background_command {
  style.stroke: "#8b949e"
}

graphics_backend -> display_list: get_next_command() {
  style.stroke: "#8b949e"
}

display_list -> graphics_backend: border_command {
  style.stroke: "#8b949e"
}

graphics_backend -> display_list: get_next_command() {
  style.stroke: "#8b949e"
}

display_list -> graphics_backend: text_command {
  style.stroke: "#8b949e"
}

graphics_backend -> layout_engine: rendering_complete {
  style.stroke: "#3fb950"
  style.bold: true
}