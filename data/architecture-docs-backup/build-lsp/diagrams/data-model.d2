direction: right

title: Core Data Model {
  near: top-center
  style.font-size: 24
  style.bold: true
  style.fill: transparent
}

Document: {
  shape: class
  uri: string
  version: number
  content: string
  "lineOffsets": "number[]"
  
  style.fill: "#1a1a2e"
  style.stroke: "#3fb950"
  style.font-color: "#e6edf3"
  style.bold: true
}

Position: {
  shape: class
  line: number
  character: number
  
  style.fill: "#1a1a2e"
  style.stroke: "#3fb950"
  style.font-color: "#e6edf3"
  style.bold: true
}

Range: {
  shape: class
  start: Position
  end: Position
  
  style.fill: "#1a1a2e"
  style.stroke: "#3fb950"
  style.font-color: "#e6edf3"
  style.bold: true
}

Symbol: {
  shape: class
  name: string
  kind: SymbolKind
  detail: string
  range: Range
  selectionRange: Range
  containerName: string
  
  style.fill: "#1a1a2e"
  style.stroke: "#3fb950"
  style.font-color: "#e6edf3"
  style.bold: true
}

Diagnostic: {
  shape: class
  range: Range
  severity: DiagnosticSeverity
  code: string
  message: string
  source: string
  
  style.fill: "#1a1a2e"
  style.stroke: "#3fb950"
  style.font-color: "#e6edf3"
  style.bold: true
}

CompletionItem: {
  shape: class
  label: string
  kind: CompletionItemKind
  detail: string
  documentation: string
  insertText: string
  range: Range
  
  style.fill: "#1a1a2e"
  style.stroke: "#3fb950"
  style.font-color: "#e6edf3"
  style.bold: true
}

Document -> Range: "contains ranges" {
  style.stroke: "#8b949e"
}

Range -> Position: "composed of" {
  style.stroke: "#8b949e"
}

Symbol -> Range: "has range selection" {
  style.stroke: "#8b949e"
}

Diagnostic -> Range: "applies to" {
  style.stroke: "#8b949e"
}

CompletionItem -> Range: "may specify" {
  style.stroke: "#8b949e"
}

Document -> Symbol: "defines symbols" {
  style.stroke: "#8b949e"
}

Document -> Diagnostic: "may have diagnostics" {
  style.stroke: "#8b949e"
}
