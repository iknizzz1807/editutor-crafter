direction: down

title: Symbol Resolution Process {
  near: top-center
  style.font-size: 18
  style.bold: true
  style.font-color: "#e6edf3"
}

start: Start {
  shape: circle
  style.fill: "#3fb950"
  style.font-color: "#000000"
  style.bold: true
}

find_identifier: Find Identifier\nat Position {
  style.fill: "#1a1a2e"
  style.stroke: "#3fb950"
  style.font-color: "#e6edf3"
}

walk_ast: Walk Up AST\nto Find Enclosing\nScopes {
  style.fill: "#1a1a2e"
  style.stroke: "#3fb950"
  style.font-color: "#e6edf3"
}

has_scope: Has Parent\nScope? {
  shape: diamond
  style.fill: "#16213e"
  style.stroke: "#3fb950"
  style.font-color: "#e6edf3"
}

search_scope: Search Current\nScope for Matching\nDeclarations {
  style.fill: "#1a1a2e"
  style.stroke: "#3fb950"
  style.font-color: "#e6edf3"
}

found_symbol: Symbol\nFound? {
  shape: diamond
  style.fill: "#16213e"
  style.stroke: "#3fb950"
  style.font-color: "#e6edf3"
}

return_symbol: Return Symbol\nInformation {
  style.fill: "#0f3460"
  style.stroke: "#3fb950"
  style.font-color: "#e6edf3"
  style.bold: true
}

move_up: Move to\nParent Scope {
  style.fill: "#1a1a2e"
  style.stroke: "#8b949e"
  style.font-color: "#e6edf3"
}

not_found: Symbol\nNot Found {
  style.fill: "#d63031"
  style.stroke: "#e17055"
  style.font-color: "#ffffff"
  style.bold: true
}

start -> find_identifier

find_identifier -> walk_ast

walk_ast -> has_scope

has_scope -> search_scope: Yes {
  style.stroke: "#3fb950"
}

has_scope -> not_found: No {
  style.stroke: "#d63031"
}

search_scope -> found_symbol

found_symbol -> return_symbol: Yes {
  style.stroke: "#3fb950"
}

found_symbol -> move_up: No {
  style.stroke: "#8b949e"
}

move_up -> has_scope