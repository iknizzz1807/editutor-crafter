# Test Result State Machine
TestResult: {
  init: {
    shape: circle
    style.fill: "#3fb950"
    label: ""
  }

  PENDING: PENDING {
    style.fill: "#1a1a2e"
    style.stroke: "#3fb950"
    style.font-color: "#e6edf3"
  }

  RUNNING: RUNNING {
    style.fill: "#1a1a2e"
    style.stroke: "#e3b341"
    style.font-color: "#e6edf3"
    style.bold: true
  }

  PASSED: PASSED {
    style.fill: "#1a1a2e"
    style.stroke: "#3fb950"
    style.font-color: "#e6edf3"
    style.double-border: true
  }

  FAILED: FAILED {
    style.fill: "#1a1a2e"
    style.stroke: "#f85149"
    style.font-color: "#e6edf3"
    style.double-border: true
  }

  ERRORED: ERRORED {
    style.fill: "#1a1a2e"
    style.stroke: "#f85149"
    style.font-color: "#e6edf3"
    style.double-border: true
  }

  SKIPPED: SKIPPED {
    style.fill: "#1a1a2e"
    style.stroke: "#8b949e"
    style.font-color: "#e6edf3"
    style.double-border: true
  }

  init -> PENDING
  PENDING -> RUNNING: test_started
  PENDING -> SKIPPED: test_skipped
  RUNNING -> PASSED: assertion_passed
  RUNNING -> FAILED: assertion_failed
  RUNNING -> ERRORED: exception_raised
  RUNNING -> SKIPPED: test_skipped
}