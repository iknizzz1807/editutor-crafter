hash_table_slot: Hash Table Slot Probe State Machine {
  style.fill: "#1a1a2e"
  style.stroke: "#3fb950"
  style.font-color: "#e6edf3"

  init: {
    shape: circle
    style.fill: "#3fb950"
    label: ""
    style.stroke-width: 0
  }

  EMPTY: {
    shape: circle
    style.fill: "#1a1a2e"
    style.stroke: "#3fb950"
    label: "EMPTY\n(Initial State)"
  }

  OCCUPIED: {
    shape: circle
    style.fill: "#1a1a2e"
    style.stroke: "#3fb950"
    label: "OCCUPIED\nKey present"
    style.bold: true
  }

  TOMBSTONE: {
    shape: circle
    style.fill: "#1a1a2e"
    style.stroke: "#8b949e"
    label: "TOMBSTONE\nKey removed"
  }

  init -> EMPTY: Start

  EMPTY -> OCCUPIED: Insert
  TOMBSTONE -> OCCUPIED: Insert

  OCCUPIED -> TOMBSTONE: Delete

  OCCUPIED -> OCCUPIED: "Lookup\nCheck key match"
}