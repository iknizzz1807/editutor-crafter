shape: sequence_diagram

orchestrator: Orchestrator {
  style.fill: "#1a1a2e"
  style.font-color: "#e6edf3"
  style.stroke: "#3fb950"
}

corpus_manager: Corpus Manager {
  style.fill: "#1a1a2e"
  style.font-color: "#e6edf3"
  style.stroke: "#3fb950"
}

mutation_engine: Mutation Engine {
  style.fill: "#1a1a2e"
  style.font-color: "#e6edf3"
  style.stroke: "#3fb950"
}

executor: Executor {
  style.fill: "#1a1a2e"
  style.font-color: "#e6edf3"
  style.stroke: "#3fb950"
}

coverage_tracker: Coverage Tracker {
  style.fill: "#1a1a2e"
  style.font-color: "#e6edf3"
  style.stroke: "#3fb950"
}

orchestrator -> corpus_manager: get_next_seed()
corpus_manager -> orchestrator: return base_input
orchestrator -> mutation_engine: mutate(base_input)
mutation_engine -> orchestrator: return mutated_input
orchestrator -> executor: run_target(mutated_input)
executor -> coverage_tracker: record_execution()
coverage_tracker -> executor: return execution_info
executor -> orchestrator: return result + coverage
orchestrator -> coverage_tracker: analyze_coverage(result)
coverage_tracker -> orchestrator: return new_paths_found
orchestrator -> corpus_manager: update_corpus(mutated_input, coverage)
corpus_manager -> orchestrator: corpus_updated