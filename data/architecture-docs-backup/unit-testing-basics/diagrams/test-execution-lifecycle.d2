shape: sequence_diagram

test_runner: Test Runner
test_discovery: Test Discovery
test_case: Test Case
setup_teardown: Setup/Teardown
assertion_engine: Assertion Engine
result_collector: Result Collector
reporter: Reporter

test_runner -> test_discovery: discover tests
test_discovery -> test_runner: test list

test_runner -> test_case: initialize test
test_case -> setup_teardown: run setup
setup_teardown -> test_case: setup complete

test_case -> test_case: execute test logic
test_case -> assertion_engine: verify expectations
assertion_engine -> test_case: assertion results

test_case -> setup_teardown: run teardown
setup_teardown -> test_case: teardown complete

test_case -> result_collector: collect test result
result_collector -> test_case: result recorded

test_runner -> result_collector: get all results
result_collector -> test_runner: aggregated results

test_runner -> reporter: generate report
reporter -> test_runner: report complete

classes: {
  primary: {
    style.fill: "#1a1a2e"
    style.stroke: "#3fb950"
    style.font-color: "#e6edf3"
    style.bold: true
  }
  
  support: {
    style.fill: "#16213e"
    style.stroke: "#8b949e"
    style.font-color: "#e6edf3"
  }
}

test_runner.class: primary
test_case.class: primary
assertion_engine.class: primary
result_collector.class: primary

test_discovery.class: support
setup_teardown.class: support
reporter.class: support