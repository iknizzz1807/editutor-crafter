direction: right

style: {
  fill: "#1a1a2e"
  stroke: "#3fb950"
  font-color: "#e6edf3"
}

title: Browser Rendering Pipeline {
  near: top-center
  style.font-size: 18
  style.bold: true
  style.fill: "#1a1a2e"
  style.font-color: "#e6edf3"
}

# Input data
html_text: HTML Text {
  shape: page
  style.fill: "#16213e"
  style.stroke: "#3fb950"
}

css_text: CSS Text {
  shape: page
  style.fill: "#16213e"
  style.stroke: "#3fb950"
}

# Processing stages
html_parser: HTML Parse {
  shape: rectangle
  style.fill: "#0f3460"
  style.stroke: "#3fb950"
  style.bold: true
}

css_parser: CSS Parse {
  shape: rectangle
  style.fill: "#0f3460"
  style.stroke: "#3fb950"
  style.bold: true
}

layout_engine: Layout {
  shape: rectangle
  style.fill: "#0f3460"
  style.stroke: "#3fb950"
  style.bold: true
}

render_engine: Render {
  shape: rectangle
  style.fill: "#0f3460"
  style.stroke: "#3fb950"
  style.bold: true
}

# Data structures
dom_tree: DOM Tree {
  shape: circle
  style.fill: "#16213e"
  style.stroke: "#8b949e"
}

styled_tree: Styled Tree {
  shape: circle
  style.fill: "#16213e"
  style.stroke: "#8b949e"
}

layout_tree: Layout Tree {
  shape: circle
  style.fill: "#16213e"
  style.stroke: "#8b949e"
}

display_list: Display List {
  shape: circle
  style.fill: "#16213e"
  style.stroke: "#8b949e"
}

# Final output
canvas: Canvas {
  shape: rectangle
  style.fill: "#16213e"
  style.stroke: "#3fb950"
  style.bold: true
}

# Flow connections
html_text -> html_parser: parse
html_parser -> dom_tree: creates
css_text -> css_parser: parse
css_parser -> styled_tree: creates {style.stroke: "#8b949e"}
dom_tree -> layout_engine: input
styled_tree -> layout_engine: styling rules
layout_engine -> layout_tree: creates
layout_tree -> render_engine: input
render_engine -> display_list: creates
display_list -> canvas: paint