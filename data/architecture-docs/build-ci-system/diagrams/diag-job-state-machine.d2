classes: {
  state: {
    shape: rectangle
    style.stroke: "#3fb950"
    style.fill: "#1a1a2e"
    style.font-color: "#e6edf3"
    style.bold: true
  }
  terminal_state: {
    shape: rectangle
    style.stroke: "#3fb950"
    style.fill: "#1a1a2e"
    style.font-color: "#e6edf3"
    style.bold: true
    style.double-border: true
  }
}

Pending.class: state
Running.class: state
Succeeded.class: terminal_state
Failed.class: terminal_state
Cancelled.class: terminal_state
Skipped.class: terminal_state

init -> Pending
Pending -> Running: start
Running -> Succeeded: finish(success)
Running -> Failed: finish(error)
Pending -> Skipped: cancel
Running -> Cancelled: cancel