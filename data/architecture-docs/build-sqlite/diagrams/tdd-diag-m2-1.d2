direction: down
ASTNode: {
  shape: class
  style: {
    fill: "#E3F2FD"
    stroke: "#1976D2"
    font-color: "#1565C0"
  }
  +type: ASTNodeType
  +location: SourceLocation
  +ast_free(node: *ASTNode)
  +ast_clone(node: *ASTNode): *ASTNode
}
SELECT_STMT: {
  shape: class
  style: {
    fill: "#E8F5E9"
    stroke: "#388E3C"
    font-color: "#2E7D32"
  }
  +columns: *ASTNode
  +table_name: char*
  +alias: char*
  +where: *ASTNode
  +group_by: *ASTNode
  +having: *ASTNode
  +order_by: *ASTNode
  +limit: int
  +offset: int
  +distinct: bool
}
INSERT_STMT: {
  shape: class
  style: {
    fill: "#E8F5E9"
    stroke: "#388E3C"
    font-color: "#2E7D32"
  }
  +table_name: char*
  +columns: *ASTNode
  +values: *ASTNode
  +select: *ASTNode
  +on_conflict: ConflictAction
  +returning: *ASTNode
}
CREATE_STMT: {
  shape: class
  style: {
    fill: "#E8F5E9"
    stroke: "#388E3C"
    font-color: "#2E7D32"
  }
  +table_name: char*
  +if_not_exists: bool
  +column_defs: *ASTNode
  +constraints: *ASTNode
  +table_type: TableType
}
BINARY_EXPR: {
  shape: class
  style: {
    fill: "#FFF3E0"
    stroke: "#F57C00"
    font-color: "#E65100"
  }
  +left: *ASTNode
  +operator: BinaryOp
  +right: *ASTNode
  +parenthesized: bool
}
UNARY_EXPR: {
  shape: class
  style: {
    fill: "#FFF3E0"
    stroke: "#F57C00"
    font-color: "#E65100"
  }
  +operator: UnaryOp
  +operand: *ASTNode
  +prefix: bool
}
LITERAL_EXPR: {
  shape: class
  style: {
    fill: "#FFF3E0"
    stroke: "#F57C00"
    font-color: "#E65100"
  }
  +literal_type: LiteralType
  +value: LiteralValue
}
IDENTIFIER_EXPR: {
  shape: class
  style: {
    fill: "#FFF3E0"
    stroke: "#F57C00"
    font-color: "#E65100"
  }
  +name: char*
  +qualified: bool
  +table_name: char*
  +schema_name: char*
}
Legend: {
  style: {
    fill: "#FAFAFA"
    stroke: "#9E9E9E"
  }
  Statement Nodes: {
    style.fill: "#E8F5E9"
    shape: rectangle
  }
  Expression Nodes: {
    style.fill: "#FFF3E0"
    shape: rectangle
  }
}
ASTNode -> SELECT_STMT: extends
ASTNode -> INSERT_STMT: extends
ASTNode -> CREATE_STMT: extends
ASTNode -> BINARY_EXPR: extends
ASTNode -> UNARY_EXPR: extends
ASTNode -> LITERAL_EXPR: extends
ASTNode -> IDENTIFIER_EXPR: extends