direction: right
header_layout: {
  label: "B-tree Page Header (8 bytes leaf / 12 bytes internal)"
  byte_0: {
    shape: rectangle
    width: 80
    height: 60
    label: "page_type\n(1 byte)"
    style.fill: "#E3F2FD"
  }
  byte_1_2: {
    shape: rectangle
    width: 120
    height: 60
    label: "first_freeblock\n(2 bytes)"
    style.fill: "#FFF3E0"
  }
  byte_3_4: {
    shape: rectangle
    width: 120
    height: 60
    label: "cell_count\n(2 bytes)"
    style.fill: "#E8F5E9"
  }
  byte_5_6: {
    shape: rectangle
    width: 140
    height: 60
    label: "cell_content_start\n(2 bytes)"
    style.fill: "#F3E5F5"
  }
  byte_7: {
    shape: rectangle
    width: 80
    height: 60
    label: "frag_bytes\n(1 byte)"
    style.fill: "#FFEBEE"
  }
  byte_8_11: {
    shape: rectangle
    width: 140
    height: 60
    label: "right_child\n(4 bytes)\ninternal only"
    style.fill: "#FFF8E1"
    style.stroke-dash: 3
  }
  byte_0 -- byte_1_2 -- byte_3_4 -- byte_5_6 -- byte_7 -- byte_8_11
}
offset_labels: {
  off_0: {
    shape: text
    label: "0"
    style.font-size: 12
    style.bold: true
  }
  off_1: {
    shape: text
    label: "1"
    style.font-size: 12
    style.bold: true
  }
  off_3: {
    shape: text
    label: "3"
    style.font-size: 12
    style.bold: true
  }
  off_5: {
    shape: text
    label: "5"
    style.font-size: 12
    style.bold: true
  }
  off_7: {
    shape: text
    label: "7"
    style.font-size: 12
    style.bold: true
  }
  off_8: {
    shape: text
    label: "8"
    style.font-size: 12
    style.bold: true
  }
  off_12: {
    shape: text
    label: "12"
    style.font-size: 12
    style.bold: true
  }
  off_0 -- off_1 -- off_3 -- off_5 -- off_7 -- off_8 -- off_12
}
legend: {
  shape: rectangle
  label: "Page Type Values:\n• 0x02: Index Internal\n• 0x05: Table Internal\n• 0x0A: Index Leaf\n• 0x0D: Table Leaf\n\nNote: cell_content_start = 0\nmeans 65536 (page size)"
  style.fill: "#FAFAFA"
  style.stroke: "#CCCCCC"
}
sizes: {
  shape: rectangle
  label: "Header Sizes:\n• Leaf pages: 8 bytes\n• Internal pages: 12 bytes\n• All values big-endian"
  style.fill: "#FAFAFA"
  style.stroke: "#CCCCCC"
}
header_layout -- offset_labels: {style.stroke: "#FFFFFF"}
header_layout -- legend: {style.stroke: "#FFFFFF"}
legend -- sizes: {style.stroke: "#FFFFFF"}