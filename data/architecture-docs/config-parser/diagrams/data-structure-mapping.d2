Data Structure Transformation {
  style.fill: "#0f3460"
  style.stroke: "#3fb950"
  style.font-color: "#e6edf3"
  style.bold: true

  ini_format: INI Format Structure {
    style.fill: "#1a1a2e"
    style.stroke: "#3fb950"
    style.font-color: "#e6edf3"

    ini_section: "[database]" {
      shape: rectangle
      - "host: localhost"
      - "port: 5432"
      - "name: mydb"
      style.fill: "#16213e"
      style.stroke: "#8b949e"
      style.font-color: "#e6edf3"
    }

    ini_global: "Global Keys" {
      shape: rectangle
      - "debug: true"
      - "timeout: 30"
      style.fill: "#16213e"
      style.stroke: "#8b949e"
      style.font-color: "#e6edf3"
    }
  }

  toml_format: TOML Format Structure {
    style.fill: "#1a1a2e"
    style.stroke: "#3fb950"
    style.font-color: "#e6edf3"

    toml_table: "[database]" {
      shape: rectangle
      - "host: localhost"
      - "port: 5432"
      - "name: mydb"
      style.fill: "#16213e"
      style.stroke: "#8b949e"
      style.font-color: "#e6edf3"
    }

    toml_dotted: "server.web" {
      shape: rectangle
      - "bind: 0.0.0.0"
      - "port: 8080"
      style.fill: "#16213e"
      style.stroke: "#8b949e"
      style.font-color: "#e6edf3"
    }
  }

  yaml_format: YAML Format Structure {
    style.fill: "#1a1a2e"
    style.stroke: "#3fb950"
    style.font-color: "#e6edf3"

    yaml_block: "database:" {
      shape: rectangle
      - "  host: localhost"
      - "  port: 5432"
      - "  name: mydb"
      style.fill: "#16213e"
      style.stroke: "#8b949e"
      style.font-color: "#e6edf3"
    }

    yaml_nested: "server:" {
      shape: rectangle
      - "  web:"
      - "    bind: 0.0.0.0"
      - "    port: 8080"
      style.fill: "#16213e"
      style.stroke: "#8b949e"
      style.font-color: "#e6edf3"
    }
  }

  unified_output: Unified Dictionary Output {
    style.fill: "#1a1a2e"
    style.stroke: "#3fb950"
    style.font-color: "#e6edf3"
    style.bold: true

    output_structure: "Nested Dictionary" {
      shape: rectangle
      - "database:"
      - "  host: localhost"
      - "  port: 5432"
      - "  name: mydb"
      - "server:"
      - "  web:"
      - "    bind: 0.0.0.0"
      - "    port: 8080"
      - "debug: true"
      - "timeout: 30"
      style.fill: "#16213e"
      style.stroke: "#3fb950"
      style.font-color: "#e6edf3"
      style.bold: true
    }
  }

  transformation_process: Transformation Rules {
    style.fill: "#1a1a2e"
    style.stroke: "#8b949e"
    style.font-color: "#e6edf3"

    section_mapping: "Section to Dictionary Key" {
      shape: rectangle
      style.fill: "#16213e"
      style.stroke: "#8b949e"
      style.font-color: "#e6edf3"
    }

    dotted_expansion: "Dotted Keys to Nested Paths" {
      shape: rectangle
      style.fill: "#16213e"
      style.stroke: "#8b949e"
      style.font-color: "#e6edf3"
    }

    indent_hierarchy: "Indentation to Nesting" {
      shape: rectangle
      style.fill: "#16213e"
      style.stroke: "#8b949e"
      style.font-color: "#e6edf3"
    }
  }

  ini_format -> transformation_process: {
    label: "Parse & Map"
    style.stroke: "#8b949e"
  }

  toml_format -> transformation_process: {
    label: "Tokenize & Build"
    style.stroke: "#8b949e"
  }

  yaml_format -> transformation_process: {
    label: "Indent & Nest"
    style.stroke: "#8b949e"
  }

  transformation_process -> unified_output: {
    label: "Unified Structure"
    style.stroke: "#3fb950"
    style.bold: true
  }

  section_mapping -> unified_output: {
    label: "[section] to key"
    style.stroke: "#8b949e"
    style.stroke-dash: 3
  }

  dotted_expansion -> unified_output: {
    label: "a.b.c to nested"
    style.stroke: "#8b949e"
    style.stroke-dash: 3
  }

  indent_hierarchy -> unified_output: {
    label: "spaces to depth"
    style.stroke: "#8b949e"
    style.stroke-dash: 3
  }
}