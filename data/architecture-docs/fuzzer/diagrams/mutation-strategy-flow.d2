classes: {
  process: {
    style.fill: "#1a1a2e"
    style.stroke: "#3fb950"
    style.font-color: "#e6edf3"
  }
  decision: {
    style.fill: "#16213e"
    style.stroke: "#3fb950"
    style.font-color: "#e6edf3"
  }
  deterministic: {
    style.fill: "#0f3460"
    style.stroke: "#3fb950"
    style.font-color: "#e6edf3"
    style.bold: true
  }
  havoc: {
    style.fill: "#d63031"
    style.stroke: "#e17055"
    style.font-color: "#ffffff"
    style.bold: true
  }
}

start: Input Seed {
  shape: oval
  class: process
}

energy_check: Energy > Threshold? {
  shape: diamond
  class: decision
}

prev_results: Previous Mutations\nSuccessful? {
  shape: diamond
  class: decision
}

deterministic_stage: Deterministic Mutations {
  shape: rectangle
  class: deterministic
}

havoc_stage: Havoc Mutations {
  shape: rectangle
  class: havoc
}

bit_flip: Bit Flip {
  class: process
}

byte_flip: Byte Flip {
  class: process
}

arithmetic: Arithmetic {
  class: process
}

splice: Splice/Insert {
  class: havoc
}

random_flip: Random Flip {
  class: havoc
}

dictionary: Dictionary {
  class: havoc
}

execution: Execute Test {
  shape: oval
  class: process
}

start -> energy_check

energy_check -> prev_results: High
energy_check -> deterministic_stage: Low

prev_results -> havoc_stage: Yes
prev_results -> deterministic_stage: No

deterministic_stage -> bit_flip
deterministic_stage -> byte_flip
deterministic_stage -> arithmetic

havoc_stage -> splice
havoc_stage -> random_flip
havoc_stage -> dictionary

bit_flip -> execution
byte_flip -> execution
arithmetic -> execution
splice -> execution
random_flip -> execution
dictionary -> execution