classes: {
  decision: {
    style.fill: "#1a1a2e"
    style.stroke: "#3fb950"
    style.font-color: "#e6edf3"
  }
  process: {
    style.fill: "#16213e"
    style.stroke: "#3fb950"
    style.font-color: "#e6edf3"
  }
  terminal: {
    style.fill: "#0f3460"
    style.stroke: "#3fb950"
    style.font-color: "#e6edf3"
    style.bold: true
  }
}

start: Start {
  shape: oval
  class: terminal
}

parse_value: Parse Value {
  class: process
}

check_token: Check Current Token {
  shape: diamond
  class: decision
}

parse_object: Parse Object {
  class: process
}

parse_array: Parse Array {
  class: process
}

parse_string: Parse String {
  class: process
}

parse_number: Parse Number {
  class: process
}

parse_literal: Parse Literal\n(true/false/null) {
  class: process
}

recursive_call: Recursive Call\nparse_value() {
  class: process
  style.stroke-dash: 3
}

success: Success\nReturn AST Node {
  shape: oval
  class: terminal
}

error: Error\nThrow ParseException {
  shape: oval
  style.fill: "#d63031"
  style.stroke: "#e17055"
  style.font-color: "#ffffff"
}

start -> parse_value
parse_value -> check_token

check_token -> parse_object: "{"
check_token -> parse_array: "["
check_token -> parse_string: "\""
check_token -> parse_number: digit
check_token -> parse_literal: true/false/null
check_token -> error: unexpected

parse_object -> recursive_call: for each value
parse_array -> recursive_call: for each element

recursive_call -> parse_value: call

parse_object -> success: complete
parse_array -> success: complete
parse_string -> success: complete
parse_number -> success: complete
parse_literal -> success: complete