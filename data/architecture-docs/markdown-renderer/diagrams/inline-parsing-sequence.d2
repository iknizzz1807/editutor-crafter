shape: sequence_diagram

parser: Inline Parser
stack: Delimiter Stack  
builder: AST Builder

parser -> stack: push('**', position=5)
stack -> parser: delimiter_added

parser -> stack: push('*', position=21)
stack -> parser: delimiter_added

parser -> stack: find_matching('*', position=28)
stack -> parser: match_found(position=21)

parser -> builder: create_emphasis_node(start=21, end=28)
builder -> parser: emphasis_node

parser -> stack: pop('*', position=21)
stack -> parser: delimiter_removed

parser -> stack: find_matching('**', position=41)
stack -> parser: match_found(position=5)

parser -> builder: create_strong_node(start=5, end=41)
builder -> parser: strong_node

parser -> stack: pop('**', position=5)
stack -> parser: delimiter_removed

parser -> builder: finalize_inline_tree()
builder -> parser: complete_ast

classes: {
  dark_theme: {
    style.fill: "#1a1a2e"
    style.stroke: "#3fb950"
    style.font-color: "#e6edf3"
  }
}

parser.class: dark_theme
stack.class: dark_theme
builder.class: dark_theme